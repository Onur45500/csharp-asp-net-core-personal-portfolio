@model Education

<div class="card mb-4 education-card">
    <div class="card-body">
        <h5 class="card-title">@Model.Degree</h5>
        <h6 class="card-subtitle mb-2 text-muted">@Model.Institution</h6>
        <p class="card-text text-muted">@Model.Duration</p>
        <p class="card-text">@Model.Description</p>
    </div>
</div>
